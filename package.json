{
  "name": "@redon2inc/strapi-plugin-pdf-creator",
  "description": "Strapi Plugin to Generate PDF documents from the Admin panel.",
  "license": "MIT",
  "author": "Redon2 Solutions <info@redon2.ca>",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "repository": {
    "url": "git+https://github.com/redon2/strapi-plugin-pdf-creator.git",
    "type": "git",
    "directory": "."
  },
  "version": "0.1.3",
  "keywords": [
    "strapi",
    "plugin",
    "pdf",
    "template"
  ],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test": "jest",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.12",
    "@strapi/icons": "^2.0.0-rc.12",
    "pdf-lib": "^1.17.1",
    "react-intl": "^6.8.6"
  },
  "devDependencies": {
    "@strapi/helper-plugin": "^5.0.0-alpha.2",
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.2.0",
    "@strapi/typescript-utils": "^5.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "styled-components": "^6.1.13",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "styled-components": "^6.1.13"
  },
  "strapi": {
    "kind": "plugin",
    "name": "strapi-plugin-pdf-creator",
    "displayName": "PDF Creator",
    "description": "Simple PDF generator based on a template and content."
  }
}
